{
  "name": "naumanni",
  "version": "0.2.0",
  "main": "index.js",
  "private": true,
  "author": "Shin ADACHI <shn@glucose.jp>",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "run-s clean copy:js build:*",
    "build:js": "webpack",
    "build:css": "postcss src/css/main.css -d static",
    "clean": "rm -f static/main.bundle.js static/main.css static/main.bundle.js.map static/*.LICENSE static/*.gz static/*.chunked.* node_modules/.cache/babel-loader/*",
    "copy:js": "cp node_modules/openpgp/dist/openpgp*.js static",
    "lint": "eslint --config .eslintrc.yml --cache ./src/js/**/*.es6",
    "lint:fix": "eslint --fix --ext=es6 --config .eslintrc.yml src/",
    "test": "run-s build test:js test:flow",
    "test:js": "jest --runInBand --coverage",
    "test:flow": "$(npm bin)/flow",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "webpack --progress --colors --watch",
    "watch:css": "postcss src/css/main.css -d static -w",
    "watch:storybook_css": "postcss src/css/storybook.css -d static -w",
    "start-storybook": "start-storybook -s ./static,./ -p 6006",
    "storybook": "npm-run-all -p watch:storybook_css start-storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "almin": "^0.11.0",
    "base65536": "^1.0.65536",
    "bezier-easing": "^2.0.3",
    "classnames": "^2.2.5",
    "crc-32": "^1.0.2",
    "emojione": "^2.2.7",
    "exif-js": "^2.2.1",
    "history": "^4.6.1",
    "htmlparser2": "^3.9.2",
    "immutability-helper": "^2.1.2",
    "immutable": "^3.8.1",
    "moment": "^2.18.1",
    "normalize.css": "^6.0.0",
    "normalizr": "^3.2.2",
    "openpgp": "^2.5.4",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.5.8",
    "raven-js": "^3.15.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-immutable-proptypes": "^2.1.0",
    "react-intl": "^2.3.0",
    "react-motion": "^0.5.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-textarea-autosize": "^4.3.2",
    "react-toggle": "^4.0.1",
    "react-transition-group": "^1.1.3",
    "superagent": "^3.5.2",
    "superagent-prefix": "^0.0.2",
    "twitter-text": "^1.14.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "almin-logger": "^4.0.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "codeclimate-test-reporter": "^0.4.1",
    "enzyme": "^2.8.2",
    "enzyme-react-intl": "^1.4.5",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.48.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "mock-socket": "^6.0.4",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.17",
    "postcss-calc": "^5.3.1",
    "postcss-cli": "^3.1.1",
    "postcss-color-function": "^3.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-mixins": "^5.4.1",
    "postcss-nested": "^1.0.1",
    "postcss-simple-vars": "^3.1.0",
    "react-addons-perf": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.3.1",
    "webpack": "^2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.es6",
      "!**/__stories__/*.(js|es6)",
      "!**/__tests__/*.(js|es6)",
      "!**/locales/*.(js|es6)",
      "!**/libs/*"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "es6",
      "js"
    ],
    "testRegex": ".*\\.test\\.es6$",
    "roots": [
      "<rootDir>/src/js/"
    ],
    "transform": {
      "^.+\\.(es6|js)$": "babel-jest"
    },
    "verbose": true
  }
}
